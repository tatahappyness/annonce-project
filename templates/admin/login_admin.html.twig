
{% extends  'base.html.twig' %}

{% block title %}Login Administrateur{% endblock %}


{% block stylesheets %}

  <!-- Custom fonts for this template-->
    <link href="{{ absolute_url('/assets/vendor/fontawesome-free/css/all.min.css') }}" rel="stylesheet" />

  <link href="https://fonts.googleapis.com/css?family=Nunito:200,200i,300,300i,400,400i,600,600i,700,700i,800,800i,900,900i" rel="stylesheet">

  <!-- Custom styles for this template-->
    <link href="{{ absolute_url('/assets/css/sb-admin-2.min.css') }}" rel="stylesheet" />

	<style>
      .card-header {
          background-color: #fcd42f4a;
      }
      #wrapper{
        background-color: #777;
      }
      .card-header-admin {
       background-color:#ececec;
      }
    </style>
    
{% endblock %}

{% block body %}


<body id="page-top">

	  <!-- Page Wrapper -->
	<div id="wrapper" >


		<!-- Content Wrapper -->
		<div id="content-wrapper" style="background-color:#ececec"; class="d-flex flex-column">

		  <!-- Main Content -->
			<div id="content">
				<!-- Topbar -->
				<nav class=" card-header-admin navbar navbar-expand navbar-light topbar mb-4 static-top shadow">
	  
				  <a class="sidebar-brand d-flex align-items-center justify-content-center" href="/">    
					<img class="" src="{{ absolute_url('/assets/img/logo.png') }}" />
				  </a>
				  
				  <!-- Sidebar Toggle (Topbar) -->
				  <button id="sidebarToggleTop" class="btn btn-link d-md-none rounded-circle mr-3">
					<i class="fa fa-bars"></i>
				  </button>            
				  

				  <!-- Topbar Navbar -->
				  <ul class="navbar-nav ml-auto">

                    <div class="navbar-nav">
                        <span class="nav-item active">
                        <a class="nav-link"><span class="d-flex align-content-baseline"><span class="lnr lnr-phone-handset text-wite bg-warning p-1"></span> <span class="bg-darkgray p-1 text-nowrap" style="font-size: 15px;">(102) 6666 8888</span></span></a>
                        </span>
                        <span class="nav-item">
                        <a class="nav-link" ><span class="d-flex align-content-baseline"><span class="lnr lnr-envelope text-wite bg-warning p-1"></span> <span class="bg-darkgray p-1 text-nowrap text-lowercase" style="font-size: 15px;">info@thememove.com</span></span></a>
                        </span>
                    </div>

					<!-- Nav Item - Search Dropdown (Visible Only XS) -->
					<li class="nav-item dropdown no-arrow d-sm-none">
					  <a class="nav-link dropdown-toggle" href="#" id="searchDropdown" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
						<i class="fas fa-search fa-fw"></i>
					  </a>
					  <!-- Dropdown - Messages -->
					  <div class="dropdown-menu dropdown-menu-right p-3 shadow animated--grow-in" aria-labelledby="searchDropdown">
						<form class="form-inline mr-auto w-100 navbar-search">
						  <div class="input-group">
							<input type="text" class="form-control bg-light border-0 small" placeholder="Search for..." aria-label="Search" aria-describedby="basic-addon2">
							<div class="input-group-append">
							  <button class="btn btn-primary" type="button">
								<i class="fas fa-search fa-sm"></i>
							  </button>
							</div>
						  </div>
						</form>
					  </div>
					</li>

					
					<div class="topbar-divider d-none d-sm-block"></div>


				  </ul>

				</nav>
			  
			  
				<!-- Begin Page Content -->
				<div class="container-fluid">
				 
				  <!-- Page Heading -->
				  <div class="d-sm-flex align-items-center justify-content-between mb-4">
					<h1 class="h3 mb-0 text-gray-800">SE CONNECTER EN TANT QUE ADMINISTRATEUR</h1>               
				  </div>
				  <!-- End Page Heading -->
				  
				  <!-- Divider -->
				  <hr class="sidebar-divider">
				  
				  <div class="row ">
				  
					<div class="col-lg-4 mb-0"></div>
					
					<!-- Content Column -->                            
					<div class="col-lg-4 mb-12">
					  <!-- form numero email logo -->
					  <div class="card shadow mb-4">
						<div class="card-header py-3">
						  <h6 class="m-0 font-weight-bold text-primary">Administrateur</h6>
						</div>

						<div class="card-body card-header-admin">
						 
							<form class="user text-gray-100">
								
								<div class="form-group row">
								  <div class="col-sm-12 mb-3 mb-sm-0">                          
									<h4 class="small  font-weight-bold">Email :</h4>
								  </div>
								  <div class="col-sm-12 mb-3 mb-sm-0">                          
									<input  type="email" name="mail" id="mail" class="form-control form-control-user" id="exampleFirstName" placeholder="Veuillez inserer votre E-mail ...">
								  </div>                                                  
								</div>


								<div class="form-group row">
								  <div class="col-sm-12 mb-3 mb-sm-0">
									<h4 class="small  font-weight-bold">Mot de passe :</h4>
								  </div>
								  <div class="col-sm-12 mb-3 mb-sm-0">                          
									<input type="password" name="pass" class="form-control form-control-user" id="pass" placeholder="Veuillez inserer votre mot de passe ...">
								  </div>
								</div>
                                
								<hr>
								
								<div class="form-group row">
								  <div class="col-sm-6 mb-3 mb-sm-0">
									<a href="/" class="btn btn-info btn-user btn-block" alt="Visualiser le resultat">
									  Retour a l'accueil
									</a>
								  </div>
								  <div class="col-sm-6">                      
									<a href="#" class="btn btn-primary btn-user btn-block" alt="Visualiser le resultat">
									  Se connecter
									</a>    
								  </div>
								</div>
								
							  </form>
						 
					   </div>
					  </div>
					</div>
					
					<div class="col-lg-4 mb-0"></div>

					<!-- End Content Column -->   
				  </div>     
						  
				</div>
			</div>
			<!-- Content Wrapper-->
		</div>
	</div>
    
</body>
	{% endblock %}

	{% block javascripts %}

  <!-- Bootstrap core JavaScript-->
  <script src="{{ absolute_url('/assets/vendor/jquery/jquery.min.js') }}"></script>
  <script src="{{ absolute_url('/assets/vendor/bootstrap/js/bootstrap.bundle.min.js') }}"></script>

  <!-- Core plugin JavaScript-->
  <script src="{{ absolute_url('/assets/vendor/jquery-easing/jquery.easing.min.js') }}"></script>

  <!-- Custom scripts for all pages-->
  <script src="{{ absolute_url('/assets/js/sb-admin-2.min.js') }}"></script>


	{% endblock %}