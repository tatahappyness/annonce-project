    <style>
        /* Set the size of the div element that contains the map */
        #map1, #map2 {
            height: 400px;  /* The height is 400 pixels */
            width: 100%;  /* The width is the width of the web page */
        }
        /** CSS THEMES
            warning dark : rgb(255, 144, 0)
            warning claire : #ffc107
            gray dark: rgb(119, 119, 119) ou #777
            gray claire : rgb(240, 240, 240) ou #bfbfbf
            white claire : rgb(248, 248, 248) ou #ffffff
            black claire: rgba(0, 0, 0, 95%)
            black dark : #000000

        **/
        .navbar, .bg-grey {
            background: {{ themesColor['HEADER']['HEADER_FONT_COUL_1'].Color != null ? themesColor['HEADER']['HEADER_FONT_COUL_1'].Color : 'rgb(240, 240, 240)' }} !important;
            color: {{ themesColor['HEADER']['HEADER_TEXT_COUL_1'].Color != null ? themesColor['HEADER']['HEADER_TEXT_COUL_1'].Color : 'rgb(248, 248, 248)' }} !important;
        }
        .navbar-mega, .sidepanel, .bg-darkgray {
            background: {{ themesColor['HEADER']['HEADER_FONT_COUL_2'].Color != null ? themesColor['HEADER']['HEADER_FONT_COUL_2'].Color : '#777'}} !important;
            color: {{ themesColor['HEADER']['HEADER_TEXT_COUL_2'].Color != null ? themesColor['HEADER']['HEADER_TEXT_COUL_1'].Color : 'rgb(248, 248, 248)' }} !important;
        }

        button, .bg-warning, .icon-sondage, .triangle {
            background: {{ themes['BUTTON_FOND_COLOR'].colorFond != null ? themes['BUTTON_FOND_COLOR'].colorFond : 'rgb(255, 144, 0)' }} !important;
            color: {{ themes['BUTTON_TEXT_COLOR'].colorFond != null ? themes['BUTTON_TEXT_COLOR'].colorFond : ' rgb(248, 248, 248)' }} !important;
        }
        .triangle::after {
            border-color: transparent transparent {{ themes['BUTTON_FOND_COLOR'].colorFond != null ? themes['BUTTON_FOND_COLOR'].colorFond : 'rgb(255, 144, 0)' }} {{ themes['BUTTON_FOND_COLOR'].colorFond != null ? themes['BUTTON_FOND_COLOR'].colorFond : 'rgb(255, 144, 0)' }} !important;
        }

        .man-young-container .text-title {
            color: {{ themesColor['TEXT_TITLE']['TEXT_TITLE_COLOR_3'].Color != null ? themesColor['TEXT_TITLE']['TEXT_TITLE_COLOR_3'].Color : ' #ffc107' }} !important;
        }
        .man-young-container .text-desc {
            color: {{ themesColor['TEXT_DESCRIPTION']['TEXT_DESCRIPTION_COLOR_1'].Color != null ? themesColor['TEXT_DESCRIPTION']['TEXT_DESCRIPTION_COLOR_1'].Color : ' rgb(248, 248, 248)' }} !important;
        }
        .text-dark {
            color: {{ themesColor['TEXT_TITLE']['TEXT_TITLE_COLOR_2'].Color != null ? themesColor['TEXT_TITLE']['TEXT_TITLE_COLOR_2'].Color : ' #ffc107' }} !important;
        }
        p, .text-secondary {
            color: {{ themesColor['TEXT_DESCRIPTION']['TEXT_DESCRIPTION_COLOR_2'].Color != null ? themesColor['TEXT_DESCRIPTION']['TEXT_DESCRIPTION_COLOR_2'].Color : ' rgb(248, 248, 248)' }} !important;
        }
        .text-warning {
            color: {{ themesColor['TEXT_TITLE']['TEXT_TITLE_COLOR_3'].Color != null ? themesColor['TEXT_TITLE']['TEXT_TITLE_COLOR_3'].Color : ' #ffc107' }} !important;
        }
        .text-white {
            color: {{ themesColor['TEXT_TITLE']['TEXT_TITLE_COLOR_1'].Color != null ? themesColor['TEXT_TITLE']['TEXT_TITLE_COLOR_1'].Color : ' #ffffff' }} !important;
        }
        .page-header-image {
            background-blend-mode: overlay !important;
            background-color: {{ themes['BUTTON_FOND_COLOR'].colorFond != null ? themes['BUTTON_FOND_COLOR'].colorFond : 'rgba(255, 144, 0, 50%)' }} !important;
        }
        .section-footer {
            background-image: url(/uploads/themes/{{ themes['FOOTER'].imageFond != null ? themes['FOOTER'].imageFond : 'footer_bg.jpg'}}) !important;
            background-color: {{ themes['FOOTER'].colorFond != null ? themes['FOOTER'].colorFond : 'rgba(19, 19, 19, 95%)'}}) !important;
            background-blend-mode: overlay !important;
        }
        
    </style>

    {% endblock %}

    {% block body %}

    <div class="page-header page-header-image-gallery page-headers-pros">
        <div class="page-header-image animated pulse" data-parallax="true" style="background-image: url('/uploads/themes/{{ themes['SPACE_PRO'].imageFond != null ? themes['SPACE_PRO'].imageFond : 'slide-inscription.jpg' }}'); background-position: 0 50px !important;">
        </div>

        <!--Begin heder-menu pro space -->
        {% include "./pro/menu/header-menu.html.twig" %}
        <!-- End header-menu pro space -->

        <!-- Begin for row content header parallax -->
        <div class="row d-flex justify-content-center font-weight-light">
            <!-- ############# Begin slide beside -->
            <div class="col-12 col-md-8" style="padding-top: 1em;">
                <div class="container border-0">
                    <div class="w-100 d-flex flex-column pb-4 justify-content-center">
                        <div class="h4 text-white text-uppercase">
                            Bienvenu dans l'espace professionel
                        </div>
                    </div>
                </div>
            </div>
            <!-- ############# End slide beside -->
        </div>
    </div>
    <!--END HEADER-->


    <div class="jumbotron bg-transparent">
        <main class="section-1 section-main bg-white">
            <!--Begin section  of the list ads by artisan-->
            <section class="bg-transparent">
                <div class="container">
                    <div Class="row">

                        <!-- Begin menu top pro -->
                        {% include "./pro/menu/top-menu.html.twig" %}
                        <!-- End menu top pro -->

                        <!-- Begin menu left pro -->
                        {% include "./pro/menu/left-menu.html.twig" %}
                        <!-- End menu left pro-->

                        <div class="col-12 col-sm-8 pt-4">
                            <div class="container-content-pro-space-right">
							
                                <!--BEGIN MY CORDONNE-->
                                <div class="card text-center">
                                    <div class="card-header p-2 font-weight-bold bg-darkgray">
                                        <h5 class="text-uppercase">MES COORDONNEES</h5>
                                    </div>
                                    <div class="card-body">
										
                                        <!-- Modal body -->
                                        <div class="pt-2">
                                        <form method="POST">
                                                <div class="form-group mb-3">
                                                    <label class="">Activité principal *</label>
                                                    <div class="input-group">
                                                        <div class="input-group-prepend  bg-warning text-center" style="padding: 5px;">
                                                            <span class="input-group-addons bg-transparent"><i class="lnr lnr-layers"></i></span>
                                                        </div>
                                                        <select name="" class="form-control text-muted">
                                                            <option selected>Sélectionnez une categorie</option>
                                                            <option value="volvo">Volvo</option>
                                                            <option value="audi">Audi</option>
                                                        </select>
                                                    </div>
                                                </div>
                                                <div class="form-group mb-3">
                                                    <label class="">Code Postale *</label>
                                                    <div class="input-group mb-3">
                                                        <div class="input-group-prepend bg-warning" style="padding: 5px;">
                                                            <span class="input-group-addon"><i class="lnr lnr-license"></i></span>
                                                        </div>
                                                        <input type="number" class="form-control text-muted" placeholder="Votre code Postale *">
                                                    </div>
                                                </div>
                                                <div class="form-group mb-3">
                                                    <label class="">Ville *</label>
                                                    <div class="input-group mb-3">
                                                        <div class="input-group-prepend bg-warning" style="padding: 5px;">
                                                            <span class="input-group-addon"><i class="lnr lnr-map"></i></span>
                                                        </div>
                                                        <input type="text" class="form-control" placeholder="Sélectionnez une ville">
                                                    </div>
                                                </div>
                                                <div class="form-group mb-3">
                                                    <label class="">Raison Sociale</label>
                                                    <div class="input-group mb-3">
                                                        <div class="input-group-prepend bg-warning" style="padding: 5px;">
                                                            <span class="input-group-addon"><i class="lnr lnr-apartment"></i></span>
                                                        </div>
                                                        <input type="text" class="form-control" placeholder="Nom de votre société">
                                                    </div>
                                                </div>
                                                <div class="form-group mb-3">
                                                    <label class="">Nom et Prénom</label>
                                                    <div class="input-group mb-2">
                                                        <div class="input-group-prepend bg-warning" style="padding: 5px;">
                                                            <span class="input-group-addon"><i class="lnr lnr-text-format"></i></span>
                                                        </div>
                                                        <input type="text" name="nom" class="form-control" placeholder="votre nom">
                                                    </div>
                                                    <div class="input-group mb-2">
                                                        <div class="input-group-prepend bg-warning" style="padding: 5px;">
                                                            <span class="input-group-addon"><i class="lnr lnr-text-format"></i></span>
                                                        </div>
                                                        <input type="text" name="prenom" class="form-control"  placeholder="votre Prénom">
                                                    </div>
                                                </div>
                                                <div class="form-group mb-3">
                                                    <label class="">Telephone</label>
                                                    <div class="input-group mb-3">
                                                        <div class="input-group-prepend bg-warning" style="padding: 5px;">
                                                            <span class="input-group-addon"><i class="lnr lnr-phone-handset"></i></span>
                                                        </div>
                                                        <input type="tel" class="form-control" size="40" onkeyup="" placeholder="Ex: +336 ou 00336">
                                                    </div>
                                                </div>
                                                <div class="form-group mb-3">
                                                    <label class="">Email</label>
                                                    <div class="input-group mb-3">
                                                        <div class="input-group-prepend bg-warning" style="padding: 5px;">
                                                            <span class="input-group-addon"><i class="lnr lnr-envelope"></i></span>
                                                        </div>
                                                        <input type="email" class="form-control"  placeholder="Votre e-mail">
                                                    </div>
                                                </div>
                                                <div class="form-group mb-3">
                                                    <label class="">Mot de passe</label>
                                                    <div class="input-group mb-3">
                                                        <div class="input-group-prepend bg-warning" style="padding: 5px;">
                                                            <span class="input-group-addon"><i class="lnr lnr-lock"></i></span>
                                                        </div>
                                                        <input type="password" class="form-control"  placeholder="Votre mot de passe ">
                                                    </div>
                                                </div>
                                                <div class="form-group mb-3">
                                                    <label class="">Confirmation mot de passe</label>
                                                    <div class="input-group mb-3">
                                                        <div class="input-group-prepend bg-warning" style="padding: 5px;">
                                                            <span class="input-group-addon"><i class="lnr lnr-lock"></i></span>
                                                        </div>
                                                        <input type="password" class="form-control"  placeholder="Reconfirmer votre mot de passe">
                                                    </div>
                                                </div>
                                                <div class="form-group mb-3">
                                                    <input type="hidden" id="g-recaptcha-response" name="g-recaptcha-response">
                                                    <div class="g-recaptcha" data-sitekey="6Lcr-qsUAAAAAPrCqD5Yk1iDIw9xBtzP6KSackqm"></div>
                                                </div>
                                                <button type="submit" class="btn btn-warning btn-register w-100">ENREGISTRER</button>
                                                <!--<button type="button" class="btn btn-annule btn-danger float-right" data-dismiss="modal">Annuler</button> -->
                                            </form>
                                        </div>
                                        <!-- Modal footer -->
										
                                    </div>
                                    <!-- <div class="card-footer text-muted pb-4"> -->
									 
                                    <!-- </div> -->
                                </div>
                                <!-- END MY CORDONNE -->
                            </div>
                        </div>
                    </div>
                </div>
            </section>
            <!--End section  of the list ads by artisan-->
	
            <!--Begin section the Presse Talk US -->
            <section id="" class="bg-transparent">
            <div class="container  bg-white">
                <div class="row d-flex justify-content-center py-4">
                    <div class="col-12 text-center">
                        <h5 class="text-uppercase">Restez connecté</h5>
                        <div class="text-secondary small"><p class=" subtitle ">
                            Pour être certain de ne rater aucun chantier, téléchargez l’application mobile pour suivre les projets lorsque vous êtes en déplacement.
                        </div>
                    </div>
                    <div class="col-12 col-sm-6 lg-8 text-center">
                        <img class="" src="{{ absolute_url('/assets/img/photos/app-mobile.png') }}"></img>
                    </div>
                    <div class="col-12 col-sm-6 lg-4 d-flex flex-column justify-content-center text-secondary">
                        <a class="btn btn-outline-secondary d-flex flex-row text-center align-items-center">
                            <span class="pro-icon mb-3">
                                <span class="icon-message-pro">
                                    <object type="image/svg+xml" data="{{ absolute_url('/assets/icon/apple_icon-icons.com_62745.svg') }}" class="icon-message-pro icon-bike">
                                        apple
                                    </object>
                                </span>
                            </span>
                            <span class="">
                                <span class="text-center ">Disponible sur </span>span><br>
                                <span class="font-weight-capitalize">App Store</span>span>
                            </span>
                        </a>
                        <a class="btn btn-outline-secondary d-flex flex-row align-items-center">
                            <span class="pro-icon mb-3">
                                <span class="icon-message-pro">
                                    <object type="image/svg+xml" data="{{ absolute_url('/assets/icon/Google_Play_icon-icons.com_66988.svg') }}" class="icon-message-pro icon-bike">
                                        play
                                    </object>
                                </span>
                            </span>
                            <span class="text-center ">
                                <span class="">Disponible sur </span>span><br>
                                <span class="font-weight-capitalize">Google Play</span>span>
                            </span>
                        </a>
                    </div>
                </div>
            </div>
            </section>
            <!--End section the Presse Talk US  -->

        </main>

        {% include "./pro/footer/footer.html.twig" %}

    </div>

