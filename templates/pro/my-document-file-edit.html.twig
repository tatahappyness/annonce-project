    {% endblock %}

    {% block body %}

    <div class="page-header page-header-image-gallery page-headers-pros">
        <div class="page-header-image animated pulse" data-parallax="true" style="background-image: url('/assets/img/photos/slide-inscription.jpg'); background-position: 0 50px !important;">
        </div>
		
        <!--Begin heder-menu pro space -->
        {% include "./pro/menu/header-menu.html.twig" %}
        <!-- End header-menu pro space -->

        <!-- Begin for row content header parallax -->
        <div class="row d-flex justify-content-center font-weight-light">
            <!-- ############# Begin slide beside -->
            <div class="col-12 col-md-8" style="padding-top: 1em;">
                <div class="container border-0">
                    <div class="w-100 d-flex flex-column pb-4 justify-content-center">
                        <div class="h4 text-white text-uppercase">
                            Mon espace professionel
                        </div>
                    </div>
                </div>
            </div>
            <!-- ############# End slide beside -->
        </div>
    </div>
    <!--END HEADER-->


    <div class="jumbotron bg-transparent">
        <main class="section-1 section-main bg-white">
            <!--Begin section  of the list ads by artisan-->
            <section class="bg-transparent">
                <div class="container">
                    <div Class="row">

                        <!-- Begin menu top pro -->
                        {% include "./pro/menu/top-menu.html.twig" %}
                        <!-- End menu top pro -->

                        <!-- Begin menu left pro -->
                        {% include "./pro/menu/left-menu.html.twig" %}
                        <!-- End menu left pro-->

                        <div class="col-12 col-sm-8 pt-4">
                            <div class="container-content-pro-space-right">

                                <!--BEGIN  MY COMPANY EDIT -->
                                <div class="card text-center">
                                    <div class="card-header p-2 font-weight-bold bg-darkgray">
                                        <h5 class="text-uppercase">Ajoutez quelques infos de votre entreprise </h5>
                                    </div>
                                    <div class="card-body">
                                        <div class="pt-2">
                                        <form id="form-company-edit">

                                                <div class="form-group mb-3">
                                                    <label class="">Date de création de votre entreprise</label>
                                                <input type="text" id="company_datecrea" class="form-control document_title" name="company_datecrea"  placeholder="EX: Janvier 2002">
                                                </div>
                                                <div class="form-group mb-3">
                                                    <label class="">Caracteristique de vote entreprise</label>
                                                    <input type="text" id="company_caracter" class="form-control" name="company_caracter"  placeholder="Ex: Entreprise Individuel">
                                                </div>
                                                <div class="form-group mb-3">
                                                    <label class=""> Description de votre entreprise </label>
                                                    <textarea  id="company_description" class="form-control message" name="company_description"  placeholder="Ecrivez quelques description de votre entreprise... " style="max-height: 150px !important; height: 110px !important; border-radius: 32px; font-size: 0.8em; border-radius: 4px; border: 1px solid silver !important;"></textarea>
                                                </div>
												<div class="form-group mb-3">
                                                    <input type="hidden" id="g-recaptcha-response" name="g-recaptcha-response">
                                                    <div class="g-recaptcha" data-sitekey="6Lcr-qsUAAAAAPrCqD5Yk1iDIw9xBtzP6KSackqm"></div>
                                                </div>
                                                <a class="btn btn-warning text-decoration-none btn-save-company-infos text-uppercase w-50">Enregistrer</a>
                                            </form>
                                        </div>
                                    </div> 
                                </div>
                                <!-- END MY COMPANY EDIT -->
							
                                <!--BEGIN MY DOCUMENT OFFICIAL-->
                                <div class="card text-center">
                                    <div class="card-header p-2 font-weight-bold bg-darkgray">
                                        <h5 class="text-uppercase">Ajouter mes documents officiels </h5>
                                    </div>
                                    <div class="card-body">
                                        <div class="pt-2">
                                        <form id="form-upload-documents" enctype="multipart/form-data">

                                                <div class="form-group mb-3">
                                                    <label class="">Titre de votre document</label>
                                                   <input type="text" id="document_title" class="form-control document_title" name="document_title"  placeholder="Titre de votre document">
                                                </div>
                                                <div class="form-group mb-1 border borber-secondary" style="height: 50px;">
                                                    <label class="text-warning">Cliquez ici ! ou Glissez vos documents ici !</label></label>
                                                    <input id="fileDocumentCompany" class="fileDocumentCompany" type="file" name="fileDocumentCompany" multiple>
												</div>
                                                <div class="w-100 border borber-secondary">
                                                    <div id="uploads" class="p-0"></div>
                                                </div>
												
                                                <div class="form-group mb-3">
                                                    <input type="hidden" id="g-recaptcha-response" name="g-recaptcha-response">
                                                    <div class="g-recaptcha" data-sitekey="6Lcr-qsUAAAAAPrCqD5Yk1iDIw9xBtzP6KSackqm"></div>
                                                </div>
                                            </form>
                                        </div>
                                    </div> 
                                    <div class="card-footer text-muted pb-4">
                                        <a href="{{path('pro_show_profil')}}" class="btn btn-warning text-decoration-none btn-retour-profile text-uppercase w-50"><< Retour</a>
                                    </div>
                                </div>
                                <!-- END MY DOCUMENT OFFICIAL -->

                                <!--BEGIN MY LABEL QUALITY-->
                                <div class="card text-center">
                                    <div class="card-header p-2 font-weight-bold bg-darkgray">
                                        <h5 class="text-uppercase">Ajouter mes labels qualités </h5>
                                    </div>
                                    <div class="card-body">
                                        <div class="pt-2">
                                        <form id="form-upload-labels-quality" enctype="multipart/form-data">

                                                <div class="form-group mb-3">
                                                    <label class="">Titre de votre label qualité</label>
                                                        <input type="text" id="label_quality_title" class="form-control label_quality_title" name="label_quality_title"  placeholder="Titre de votre label qualité">
                                                </div>
                                                <div class="form-group mb-1 border borber-secondary" style="height: 50px;">
                                                    <label class="text-warning">Cliquez ici ! ou Glissez vos labels qualités ici !</label></label>
                                                    <input id="fileLabelQuality" class="fileLabelQuality" type="file" name="file_image_label" multiple>
												</div>
                                                <div class="w-100 border borber-secondary">
                                                    <div id="uploads" class="p-0"></div>
                                                </div>
												
                                                <div class="form-group mb-3">
                                                    <input type="hidden" id="g-recaptcha-response" name="g-recaptcha-response">
                                                    <div class="g-recaptcha" data-sitekey="6Lcr-qsUAAAAAPrCqD5Yk1iDIw9xBtzP6KSackqm"></div>
                                                </div>
                                            </form>
                                        </div>
                                    </div> 
                                    <div class="card-footer text-muted pb-4">
                                        <a href="{{path('pro_show_profil')}}" class="btn btn-warning text-decoration-none btn-retour-profile text-uppercase w-50"><< Retour</a>
                                    </div>
                                </div>
                                <!-- END MY  LABEL QUALITY -->


                            </div>
                        </div>
                    </div>
                </div>
            </section>
            <!--End section  of the list ads by artisan-->
	
            <!--Begin section the Presse Talk US -->
            <section id="" class="bg-transparent">
            <div class="container  bg-white">
                <div class="row d-flex justify-content-center py-4">
                    <div class="col-12 text-center">
                        <h5 class="text-uppercase">Restez connecté</h5>
                        <div class="text-secondary small"><p class=" subtitle ">
                            Pour être certain de ne rater aucun chantier, téléchargez l’application mobile pour suivre les projets lorsque vous êtes en déplacement.
                        </div>
                    </div>
                    <div class="col-12 col-sm-6 lg-8 text-center">
                        <img class="" src="{{ absolute_url('/assets/img/photos/app-mobile.png') }}"></img>
                    </div>
                    <div class="col-12 col-sm-6 lg-4 d-flex flex-column justify-content-center text-secondary">
                        <a class="btn btn-outline-secondary d-flex flex-row text-center align-items-center">
                            <span class="pro-icon mb-3">
                                <span class="icon-message-pro">
                                    <object type="image/svg+xml" data="{{ absolute_url('/assets/icon/apple_icon-icons.com_62745.svg') }}" class="icon-message-pro icon-bike">
                                        apple
                                    </object>
                                </span>
                            </span>
                            <span class="">
                                <span class="text-center ">Disponible sur <span/><br>
                                <span class="font-weight-capitalize">App Store<span/>
                            <span/>
                        </a>
                        <a class="btn btn-outline-secondary d-flex flex-row align-items-center">
                            <span class="pro-icon mb-3">
                                <span class="icon-message-pro">
                                    <object type="image/svg+xml" data="{{ absolute_url('/assets/icon/Google_Play_icon-icons.com_66988.svg') }}" class="icon-message-pro icon-bike">
                                        play
                                    </object>
                                </span>
                            </span>
                            <span class="text-center ">
                                <span class="">Disponible sur <span/><br>
                                <span class="font-weight-capitalize">Google Play<span/>
                            <span/>
                        </a>
                    </div>
                </div>
            </div>
            </section>
            <!--End section the Presse Talk US  -->

        </main>

        {% include "./pro/footer/footer.html.twig" %}

    </div>

{% endblock %}

