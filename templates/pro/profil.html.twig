	{% extends  'base.html.twig' %}

	{% block stylesheets %}

	<!--<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css">
	<link href="https://cdnjs.cloudflare.com/ajax/libs/select2/4.0.7/css/select2.min.css" rel="stylesheet" />-->
	<script src="https://cdn.linearicons.com/free/1.0.0/svgembedder.min.js"></script>
	<link rel="stylesheet" href="https://cdn.linearicons.com/free/1.0.0/icon-font.min.css">
	<!-- CSS Files -->
	<link href="{{ absolute_url('/assets/sweetalert2/dist/sweetalert2.min.css') }}" rel="stylesheet" />
	<link href="{{ absolute_url('/assets/css/now-ui-kit.min.css') }}" rel="stylesheet" />
	<link href="{{ absolute_url('/assets/css/bootstrap.min.css') }}" rel="stylesheet" />
	<link href="{{ absolute_url('/animate/animate.min.css') }}" rel="stylesheet" />
	<link href="{{ absolute_url('/assets/loadbar/css/loading-bar.css') }}" rel="stylesheet" />
	<link href="{{ absolute_url('/assets/select2-develop/dist/css/select2.min.css') }}" rel="stylesheet" />
	<link href="{{ absolute_url('/assets/swiper/dist/css/swiper.min.css') }}" rel="stylesheet" />
	<link href="{{ absolute_url('/assets/form-multistep/form-step.css') }}" rel="stylesheet" />
	<link href="{{ absolute_url('/css/loading.css') }}" rel="stylesheet" />
	<link href="{{ absolute_url('/css/loading-btn.css') }}" rel="stylesheet" />
	<link href="{{ absolute_url('/css/style.css') }}" rel="stylesheet" />

	{% endblock %}

	{% block body %}

	<div class="page-header page-header-image-gallery">
		<div class="page-header-image animated pulse" data-parallax="true" style="background-image: url('/assets/img/photos/slide-inscription.jpg'); background-position: 0 50px !important;">
		</div>

		<!--Begin heder-menu pro space -->
		{% include "./pro/menu/header-menu.html.twig" %}
		<!-- End header-menu pro space -->

		<!-- Begin for row content header parallax -->
		<div class="row d-flex justify-content-center font-weight-light">
			<!-- ############# Begin slide beside -->
			<div class="col-12 col-md-8" style="padding-top: 1em;">
				<div class="container border-0">
					<div class="w-100 d-flex flex-column pb-4 justify-content-center">
						<div class="h4 text-white text-uppercase">
							Bienvenu dans l'espace professionel
						</div>
					</div>
				</div>
			</div>
			<!-- ############# End slide beside -->
		</div>
	</div>
	<!--END HEADER-->


	<div class="jumbotron bg-transparent">
		<main class="section-1 section-main bg-white">
			<!--Begin section  of the list ads by artisan-->
			<section class="bg-transparent">
				<div class="container">
					<div Class="row">

						<!-- Begin menu top pro -->
						{% include "./pro/menu/top-menu.html.twig" %}
						<!-- End menu top pro -->

						<!-- Begin menu left pro -->
						{% include "./pro/menu/left-menu.html.twig" %}
						<!-- End menu left pro-->

						<div class="col-12 col-sm-8 pt-4">
							<div class="container-content-pro-space-right">
								<!--BEGIN MY CORDONNE-->
								<div class="card text-center">
									<div class="card-header p-2 font-weight-bold bg-darkgray">
										<h5 class="text-uppercase">MES COORDONNÉES</h5>
									</div>
									<div class="card-body">
										<div class="row d-flex justify-content-center">
											<div class="col-12 col-md-10 text-secondary">
												<p class="small"><span class="font-weight-bold ">Nom: {{ user.getUsername() }} </span></p>
												<p class="small"><span class="font-weight-bold">Prénom: {{ user.getFirstname() }} </span></p>
												<p class="small"><span class="font-weight-bold">Téléphone: {{ user.getMobile() }} </span></p>
												<p class="small"><span class="font-weight-bold">E-mail: {{ user.getEmail() }} </span></p>
												<p class="small"><span class="font-weight-bold">Code postal: {{ user.getZipCode() }} </span></p>
												<p class="small"><span class="font-weight-bold">Adresse: {{ user.getUserCity().getVilleNomReel() }} </span></p>
												<p class="small"><span class="font-weight-bold">Mon société: {{ user.getCompanyName() }} </span></p>
												<p class="small"><span class="font-weight-bold">Date de création : {{ user.getDateCrea() | date('d/m/y H:m:s')}} </span></p>
											</div>
										</div>
									</div>
								    <div class="card-footer text-muted pb-4">
									 <a href="{{ path('pro_cordonation_edit') }}" class="btn-outline-secondary text-decoration-none text-secondary text-center" style="font-size: 28px !important; padding: 4px; font-weight: bold; border: 4px solid;border-radius: 4px;"><span class="lnr lnr-pencil text-warning" style="font-size: 28px; font-weight: bold;"></span></a>
								    </div>
								</div>
								<!-- END MY CORDONNE -->
								
								<!-- BEGIN DESRIPTION COMPANY-->
								<div class="card text-center">
									<div class="card-header p-2 font-weight-bold bg-darkgray">
										<h5 class="text-uppercase">DESCRIPTION DE MON ENTREPRISE</h5>
									</div>
									<div class="card-body">
										<div class="row">
											<div class="col-12">
												<div class="d-block"><h5 class="text-uppercase">MES DOCUMENTS OFFICIELS</h5></div>
												<div class="d-block pb-2">
													<p class="text-secondary small">
														Vous avez 2 fois plus de chance de signer un projet en important vos documents légaux !
													</p>
													<div class="d-flex flex-wrap pb-2 justify-content-center">
														
														{% if documents != null %}
															{% for document in documents %}
															<a class="m-2" href="{{ path('pro_download_file', {id: document.getName(), nameActually: document.getTitle()} )}}" title="{{ document.getTitle() }}">
																<img class="" src="{{ absolute_url('/uploads/logo/icon-pdf.jpg') }}" style="width: 50px ; height: 50px;" alt="pdf document" />
															</a>
															{% endfor %}
															{% else %}
															<span class="lnr lnr-file-empty text-secondary" style="font-size: 64px;" title="Kbis" alt="Kbis"></span>
															<span class="lnr lnr-file-empty text-secondary" style="font-size: 64px;" title="Kbis" alt="Kbis"></span>
															<span class="lnr lnr-file-empty text-secondary" style="font-size: 64px;" title="Kbis" alt="Kbis"></span>
															<span class="lnr lnr-file-empty text-secondary" style="font-size: 64px;" title="Kbis" alt="Kbis"></span>
															<span class="">	
																<a class="text-decoration-none" href="{{ path('pro_document_file_edit') }}"><span class="lnr lnr-file-add text-warning" style="font-size: 64px;" title="Kbis" alt="Kbis"></span></a>
															</span>
														{% endif %}

													</div>
												</div>
											</div>
											<div class="col-12">
												<div class="d-block"><h5 class="text-uppercase">MES LABELS QUALITÉ</h5></div>
												<div class="d-block pb-2">
													<p class="text-secondary small">
														Gagnez en visibilité et en crédibilité en important vos labels qualité !
													</p>
													<div class="d-flex flex-wrap justify-content-center">
														
														{% if labels != null %}
															{% for label in labels %}
															<a class="m-2" href="{{ path('pro_download_file', {id: label.getName()} )}}" title="{{ label.getTitle() }}">
																<img class="" src="{{ absolute_url('/uploads/profil/' ~ label.getName()) }}" style="width: 65px ; height: 65px;" alt="pdf document" />
															</a>
															{% endfor %}
															{% else %}
															<span class="lnr lnr-license text-secondary m-1" style="font-size: 64px;" title="Kbis" alt="Kbis"></span>
															<span class="lnr lnr-license text-secondary m-1" style="font-size: 64px;" title="Kbis" alt="Kbis"></span>
															<span class="lnr lnr-license text-secondary m-1" style="font-size: 64px;" title="Kbis" alt="Kbis"></span>
															<span class="lnr lnr-license text-secondary m-1" style="font-size: 64px;" title="Kbis" alt="Kbis"></span>
															
															<a href="{{ path('pro_document_file_edit') }}">
																<span class="lnr lnr-file-add text-warning m-1" style="font-size: 64px;" title="Kbis" alt="Kbis"></span>
															</a>
														{% endif %}
														
													</div>
												</div> 
											</div>
										</div>
									</div>
								    <div class="card-footer text-muted pb-4">
									 <a href="{{ path('pro_document_file_edit') }}" class="btn-outline-secondary text-text-decoration-none text-secondary text-center" style="font-size: 28px !important; padding: 4px; font-weight: bold; border: 4px solid;border-radius: 4px;"><span class="lnr lnr-pencil text-warning" style="font-size: 28px; font-weight: bold;"></span></a>
								    </div>
								</div>
								<!-- END DESRIPTION COMPANY -->
								
								<!-- BEGIN CHANTIERS RÉALISÉS IMAGES-->
								<div class="card text-center">
									<div class="card-header p-2 font-weight-bold bg-darkgray">
										<h5 class="text-uppercase">IMAGES CHANTIERS RÉALISÉS</h5>
									</div>
									<div class="card-body">
										<p class="text-secondary small">
											Gagnez en visibilité et en crédibilité en important vos images des chantiers réalisés !
										</p>
										
										<div class="row text-center text-lg-left">
											
											{% if images != null %}
												{% for image in images %}
													<div class="col-lg-3 col-md-4 col-6 p-1">
														<a href="#" class="d-block mb-4 h-100">
															<img class="img-fluid img-thumbnail" src="{{ absolute_url('/uploads/images/' ~ image.getName) }}" title="{{ image.getArticleTitle().getArticleTitle() }}" alt="">
														</a>
													</div>
												{% endfor %}
											{% endif %}
											

											<div class="col-lg-3 col-md-4 col-6 p-1">
												<a href="#" class="d-block mb-4 h-100">
													<img class="img-fluid img-thumbnail" src="https://source.unsplash.com/aob0ukAYfuI/400x300" alt="">
												</a>
											</div>
											<div class="col-lg-3 col-md-4 col-6 p-1">
												<a href="#" class="d-block mb-4 h-100">
													<img class="img-fluid img-thumbnail" src="https://source.unsplash.com/aob0ukAYfuI/400x300" alt="">
												</a>
											</div>
										</div>

									</div>
								    <div class="card-footer text-muted pb-4">
									 <a href="{{ path('pro_image_realize_edit') }}" class="btn-outline-secondary text-secondary text-center" style="font-size: 28px !important; padding: 4px; font-weight: bold; border: 4px solid;border-radius: 4px;"><span class="lnr lnr-pencil text-warning" style="font-size: 28px; font-weight: bold;"></span></a>
								    </div>
								</div>
								<!-- END CHANTIERS RÉALISÉS IMAGES -->
								
								<!-- BEGIN BEGIN CHANTIERS RÉALISÉS VIDEOS-->
								<div class="card text-center">
									<div class="card-header p-2 font-weight-bold bg-darkgray">
										<h5 class="text-uppercase">VIDEOS CHANTIERS RÉALISÉS</h5>
									</div>
									<div class="card-body">
										<p class="text-secondary small">
											Gagnez en visibilité et en crédibilité en important vos videos des chantiers réalisés!
										</p>
										<div class="d-flex flex-wrap justify-content-center">
											<span class="lnr lnr-film-play text-secondary m-1" style="font-size: 64px;" title="Kbis" alt="Kbis"></span>
											<span class="lnr lnr-film-play text-secondary m-1" style="font-size: 64px;" title="Kbis" alt="Kbis"></span>
											<span class="lnr lnr-film-play text-secondary m-1" style="font-size: 64px;" title="Kbis" alt="Kbis"></span>
											<span class="lnr lnr-film-play text-secondary m-1" style="font-size: 64px;" title="Kbis" alt="Kbis"></span>
											<span class="lnr lnr-file-add text-warning m-1" style="font-size: 64px;" title="Kbis" alt="Kbis"></span>
										</div>
									</div>
								    <div class="card-footer text-muted pb-4">
									 <a href="{{ path('pro_video_realize_edit') }}" class="btn-outline-secondary text-decoration-none text-secondary text-center" style="font-size: 28px !important; padding: 4px; font-weight: bold; border: 4px solid;border-radius: 4px;"><span class="lnr lnr-pencil text-warning" style="font-size: 28px; font-weight: bold;"></span></a>
								    </div>
								</div>
								<!-- END BEGIN CHANTIERS RÉALISÉS VIDEOS -->
								
							</div>
						</div>
					</div>
				</div>
			</section>
			<!--End section  of the list ads by artisan-->
	
			<!--Begin section the Presse Talk US -->
			<section id="" class="bg-transparent">
			<div class="container  bg-white">
				<div class="row d-flex justify-content-center py-4">
					<div class="col-12 text-center">
						<h5 class="text-uppercase">Restez connecté</h5>
						<div class="text-secondary small"><p class=" subtitle ">
							Pour être certain de ne rater aucun chantier, téléchargez l’application mobile pour suivre les projets lorsque vous êtes en déplacement.
						</div>
					</div>
					<div class="col-12 col-sm-6 lg-8 text-center">
						<img class="" src="{{ absolute_url('/assets/img/photos/app-mobile.png') }}"></img>
					</div>
					<div class="col-12 col-sm-6 lg-4 d-flex flex-column justify-content-center text-secondary">
						<a class="btn btn-outline-secondary d-flex flex-row text-center align-items-center">
							<span class="pro-icon mb-3">
								<span class="icon-message-pro">
									<object type="image/svg+xml" data="{{ absolute_url('/assets/icon/apple_icon-icons.com_62745.svg') }}" class="icon-message-pro icon-bike">
										apple
									</object>
								</span>
							</span>
							<span class="">
								<span class="text-center ">Disponible sur <span/><br>
								<span class="font-weight-capitalize">App Store<span/>
							<span/>
						</a>
						<a class="btn btn-outline-secondary d-flex flex-row align-items-center">
							<span class="pro-icon mb-3">
								<span class="icon-message-pro">
									<object type="image/svg+xml" data="{{ absolute_url('/assets/icon/Google_Play_icon-icons.com_66988.svg') }}" class="icon-message-pro icon-bike">
										play
									</object>
								</span>
							</span>
							<span class="text-center ">
								<span class="">Disponible sur <span/><br>
								<span class="font-weight-capitalize">Google Play<span/>
							<span/>
						</a>
					</div>
				</div>
			</div>
			</section>
			<!--End section the Presse Talk US  -->

		</main>

		{% include "./pro/footer/footer.html.twig" %}

	</div>

{% endblock %}

