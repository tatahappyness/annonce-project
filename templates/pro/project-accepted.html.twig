 
	{% extends  'base.html.twig' %}

	{% block stylesheets %}

	<!--<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css">
	<link href="https://cdnjs.cloudflare.com/ajax/libs/select2/4.0.7/css/select2.min.css" rel="stylesheet" />-->
	<script src="https://cdn.linearicons.com/free/1.0.0/svgembedder.min.js"></script>
	<link rel="stylesheet" href="https://cdn.linearicons.com/free/1.0.0/icon-font.min.css">
	<!-- CSS Files -->
	<link href="{{ absolute_url('/assets/sweetalert2/dist/sweetalert2.min.css') }}" rel="stylesheet" />
	<link href="{{ absolute_url('/assets/css/now-ui-kit.min.css') }}" rel="stylesheet" />
	<link href="{{ absolute_url('/assets/css/bootstrap.min.css') }}" rel="stylesheet" />
	<link href="{{ absolute_url('/animate/animate.min.css') }}" rel="stylesheet" />
	<link href="{{ absolute_url('/assets/loadbar/css/loading-bar.css') }}" rel="stylesheet" />
	<link href="{{ absolute_url('/assets/select2-develop/dist/css/select2.min.css') }}" rel="stylesheet" />
	<link href="{{ absolute_url('/assets/swiper/dist/css/swiper.min.css') }}" rel="stylesheet" />
	<link href="{{ absolute_url('/assets/form-multistep/form-step.css') }}" rel="stylesheet" />
	<link href="{{ absolute_url('/css/loading.css') }}" rel="stylesheet" />
	<link href="{{ absolute_url('/css/loading-btn.css') }}" rel="stylesheet" />
	<link href="{{ absolute_url('/css/style.css') }}" rel="stylesheet" />

	<style>
	/* Set the size of the div element that contains the map */
	#map1, #map2 {
		height: 400px;  /* The height is 400 pixels */
		width: 100%;  /* The width is the width of the web page */
	}
	</style>

	{% endblock %}

	{% block body %}

	<div class="page-header page-header-image-gallery page-headers-pros">
		<div class="page-header-image animated pulse" data-parallax="true" style="background-image: url('/assets/img/photos/slide-inscription.jpg'); background-position: 0 50px !important;">
		</div>

		<!--Begin heder-menu pro space -->
		{% include "./pro/menu/header-menu.html.twig" %}
		<!-- End header-menu pro space -->

		<!-- Begin for row content header parallax -->
		<div class="row d-flex justify-content-center font-weight-light">
			<!-- ############# Begin slide beside -->
			<div class="col-12 col-md-8" style="padding-top: 1em;">
				<div class="container border-0">
					<div class="w-100 d-flex flex-column pb-4 justify-content-center">
						<div class="h4 text-white text-uppercase">
							Mon espace professionel
						</div>
					</div>
				</div>
			</div>
			<!-- ############# End slide beside -->
		</div>
	</div>
	<!--END HEADER-->
	<div class="jumbotron bg-transparent">
		<main class="section-1 section-main bg-white">
			<!--Begin section  of the list ads by artisan-->
			<section class="bg-transparent">
				<div class="container">
					<div Class="row">
					
						<!-- Begin menu top pro -->
						{% include "./pro/menu/top-menu.html.twig" %}
						<!-- End menu top pro -->

						<!-- Begin menu left pro -->
						{% include "./pro/menu/left-menu.html.twig" %}
						<!-- End menu left pro-->

						<div class="col-12 col-sm-8 pt-4">
							<div class="container-content-pro-space-right">
								<!-- PROJECT ACCETED-->
								<div class="card text-center">
									<div class="card-header p-2 font-weight-bold bg-darkgray">
										<h5 class="text-uppercase">Mes demandes acceptés</h5>
									</div>
									<div class="card-body">
										<ul class="list-group list-group-flush text-secondary">
											{% if devisAccept != null %}
												{% for dev in devisAccept %}
													<li class="list-group-item">
														<div class="d-block">
															<div class="d-flex flex-row align-items-center justify-content-around">
																<div class="">
																	<div class="rounded-circle d-block" style="width: 80px; height: 80px; background: orange;">
																		<span class="lnr lnr-clock" style="font-size: 64px; line-height: 80px;"></span>
																	</div>
																</div>
																<div class="px-2 info-ads-detail">
																	<a href="{{ path('pro_devis_receved_detail', {id: dev.getDevisId().getId() }) }}" class="text-reset"><h3 class="d-block text-left">{{ dev.getDevisId().getCategoryId().getCategTitle() }}</h3></a>
																	<p class="d-block text-dark text-left">
																		{{ dev.getDevisId().getFirstName() }} - habite à {{ dev.getDevisId().getCity().getVilleNomReel() }} <br>
																		<span class="text-secondary">{{ dev.getDevisId().getDetailProject() }}</span>
																		<a  data-id="{{ dev.id }}" class="btn btn-danger btn-valid-devis float-right text-white text-decoration-none {% for devVald in devisValid %} {% if devVald.getDevisAcceptId().getId() == dev.id %} d-none {% endif %}{% endfor %}">Je valide</a>
																	</p>
																</div>
															</div>
														</div>
														<div class="d-block text-center float-left">Date de demande: {{  dev.getDevisId().getDatecrea() | date('d/m/y')}}</div>
													</li>
												{% endfor %}
												{% else %}
												VOUS N'AVEZ PAS ENCORE DES PROJET ACCEPTEE
											{% endif %}
										</ul>
									</div>
								    <div class="card-footer text-muted pb-4">
									 {# <a class="btn-outline-warning text-warning">Voir tous les projets</a> #}
								    </div>
								</div>
								<!-- END PROJECT ACCEPTED -->

								<!-- PROJECT VALIDED-->
								<div class="card text-center">
									<div class="card-header p-2 font-weight-bold bg-darkgray">
										<h5 clss="text-uppercase">Mes projets validés</h5>
									</div>
									<div class="card-body">
										<ul class="list-group list-group-flush text-secondary">
											
											{% if devisValid != null %}
												{% for devValid in devisValid %}
													<li class="list-group-item">
														<div class="d-block">
															<div class="d-flex flex-row align-items-center justify-content-around">
																<div class="">
																	<div class="rounded-circle d-block" style="width: 80px; height: 80px; background: orange;">
																		<span class="lnr lnr-clock" style="font-size: 64px; line-height: 80px;"></span>
																	</div>
																</div>
																<div class="px-2 info-ads-detail">
																	<a href="{{ path('pro_devis_receved_detail', {id: devValid.getDevisAcceptId().getDevisId().getId() }) }}" class="text-reset"><h3 class="d-block text-left">{{ devValid.getDevisAcceptId().getDevisId().getCategoryId().getCategTitle() }}</h3></a>
																	<p class="d-block text-dark text-left">
																		{{ devValid.getDevisAcceptId().getDevisId().getFirstName() }} - habite à {{ devValid.getDevisAcceptId().getDevisId().getCity().getVilleNomReel() }} <br>
																		<span class="text-secondary">{{ devValid.getDevisAcceptId().getDevisId().getDetailProject() }}</span>
																		<a data-id="{{ devValid.id }}" class="btn btn-success float-right btn-termine-devis text-white text-decoration-none {% for devFin in devisFinish %} {% if devFin.getDevisValid().getId() == devValid.id %} d-none {% endif %}{% endfor %}">Je termine</a>
																	</p>
																</div>
															</div>
														</div>
														<div class="d-block text-center float-left">Date de demande: {{  devValid.getDevisAcceptId().getDevisId().getDatecrea() | date('d/m/y')}}</div>
													</li>
												{% endfor %}
												{% else %}
													VOUS N'AVEZ PAS ENCORE DES PROJET VALIDEE
											{% endif %}
											
										</ul>
									</div>
									<div class="card-footer text-muted pb-4">
									{# <a class="btn-outline-warning text-warning">Voir tous les projets</a> #}
									</div>
								</div>
								<!-- END PROJECT VALIDED -->

								<!-- PROJECT TERMINÉS AVEC SUCCÈS-->
								<div class="card text-center">
									<div class="card-header p-2 font-weight-bold bg-darkgray">
										<h5 class="text-uppercase">Mes projets terminés avec succès</h5>
									</div>
									<div class="card-body">
										<ul class="list-group list-group-flush text-secondary">
											
											{% if devisFinish != null %}
												{% for devFinish in devisFinish %}
													<li class="list-group-item">
														<div class="d-block">
															<div class="d-flex flex-row align-items-center justify-content-around">
																<div class="">
																	<div class="rounded-circle d-block" style="width: 80px; height: 80px; background: orange;">
																		<span class="lnr lnr-clock" style="font-size: 64px; line-height: 80px;"></span>
																	</div>
																</div>
																<div class="px-2 info-ads-detail">
																	<a href="{{ path('pro_devis_receved_detail', {id: devFinish.getDevisValid().getDevisAcceptId().getDevisId().getId() }) }}" class="text-reset"><h3 class="d-block text-left">{{ devFinish.getDevisValid().getDevisAcceptId().getDevisId().getCategoryId().getCategTitle() }}</h3></a>
																	<p class="d-block text-dark text-left">
																		{{ devFinish.getDevisValid().getDevisAcceptId().getDevisId().getFirstName() }} - habite à {{ devFinish.getDevisValid().getDevisAcceptId().getDevisId().getCity().getVilleNomReel() }} <br>
																		<span class="text-secondary">{{ devFinish.getDevisValid().getDevisAcceptId().getDevisId().getDetailProject() }}</span>
																		{# <a data-id="{{ devFinish.id }}" class="btn btn-danger float-right text-white text-decoration-none">Supprimer</a> #}
																	</p>
																</div>
															</div>
														</div>
														<div class="d-block text-center float-left">Date de demande: {{  devFinish.getDevisValid().getDevisAcceptId().getDevisId().getDatecrea() | date('d/m/y')}}</div>
													</li>
												{% endfor %}
												{% else %}
													VOUS N'AVEZ PAS ENCORE DES PROJET VALIDEE
											{% endif %}
											
										</ul>
									</div>
								    <div class="card-footer text-muted pb-4">
									 {# <a class="btn-outline-warning text-warning">Voir tous les projets</a> #}
								    </div>
								</div>
								<!-- END PROJECT TERMINÉS AVEC SUCCÈS -->
							</div>
						</div>
					</div>
				</div>
			</section>
			<!--End section  of the list ads by artisan-->
			<!--Begin section the Presse Talk US -->
			<section id="" class="bg-transparent">
			<div class="container  bg-white">
				<div class="row d-flex justify-content-center py-4">
					<div class="col-12 text-center">
						<h5 class="text-uppercase">Restez connecté</h5>
						<div class="text-secondary small"><p class=" subtitle ">
							Pour être certain de ne rater aucun chantier, téléchargez l’application mobile pour suivre les projets lorsque vous êtes en déplacement.
						</div>
					</div>
					<div class="col-12 col-sm-6 lg-8 text-center">
						<img class="" src="{{ absolute_url('/assets/img/photos/app-mobile.png') }}"></img>
					</div>
					<div class="col-12 col-sm-6 lg-4 d-flex flex-column justify-content-center text-secondary">
						<a class="btn btn-outline-secondary d-flex flex-row text-center align-items-center">
							<span class="pro-icon mb-3">
								<span class="icon-message-pro">
									<object type="image/svg+xml" data="{{ absolute_url('/assets/icon/apple_icon-icons.com_62745.svg') }}" class="icon-message-pro icon-bike">
										apple
									</object>
								</span>
							</span>
							<span class="">
								<span class="text-center ">Disponible sur <span/><br>
								<span class="font-weight-capitalize">App Store<span/>
							<span/>
						</a>
						<a class="btn btn-outline-secondary d-flex flex-row align-items-center">
							<span class="pro-icon mb-3">
								<span class="icon-message-pro">
									<object type="image/svg+xml" data="{{ absolute_url('/assets/icon/Google_Play_icon-icons.com_66988.svg') }}" class="icon-message-pro icon-bike">
										play
									</object>
								</span>
							</span>
							<span class="text-center ">
								<span class="">Disponible sur <span/><br>
								<span class="font-weight-capitalize">Google Play<span/>
							<span/>
						</a>
					</div>
				</div>
			</div>
			</section>
			<!--End section the Presse Talk US  -->
		</main>
		
		{% include "./pro/footer/footer.html.twig" %}


	</div>

{% endblock %}

